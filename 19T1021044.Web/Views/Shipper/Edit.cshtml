@model _19T1021044.DomainModels.Shipper
@{

}


<div class=" box box-primary">
    <div class=" box-body">
        <form action="~/Shipper/Save" method="post">
            <input type="hidden" name="ShipperID" value="@Model.ShipperID" />
            <div class="form-group">
                <label>Tên Người Giao Hàng:</label>
                <input type="text" class="form-control" id="ShipperName" name="ShipperName" value="@Model.ShipperName" autofocus>
            </div>

            <div class="form-group">
                <label>Số Điện Thoại:</label>
                <input type="text" class="form-control" id="Phone" name="Phone" value="@Model.Phone" autofocus>
            </div>


            <button type="submit" class="btn btn-success"><i class="glyphicon glyphicon-floppy-disk"></i> Lưu Dữ Liệu</button>
            <a href="~/Shipper" class="btn btn-warning"> Quay Lại</a>
        </form>
    </div>
</div>

